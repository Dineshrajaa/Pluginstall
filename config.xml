<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cgvak.dineshraja.pluginstall" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Pluginstall</name>
    <description>
        A Simple Cordova app to illustrate various plugins.
    </description>
    <author email="dineshrajaa.93@gmail.com" href="http://cordova.io">
        Dinesh Raja
    </author>
    <preference name="phonegap-version" value="3.6.3" />
    <preference name="DisallowOverscroll" value="true"/>

    <content src="index.html" />
    <gap:plugin name="org.apache.cordova.battery-status" version="0.2.11" />
    <gap:plugin name="org.apache.cordova.camera" version="0.3.2" />
    <gap:plugin name="org.apache.cordova.console"/>
    <gap:plugin name="org.apache.cordova.contacts" version="0.2.13" />
    <gap:plugin name="org.apache.cordova.device" version="0.2.12" />
    <gap:plugin name="org.apache.cordova.device-motion"/>
    <gap:plugin name="org.apache.cordova.device-orientation"/>
    <gap:plugin name="org.apache.cordova.file"/>
    <gap:plugin name="org.apache.cordova.file-transfer"/>
    <gap:plugin name="org.apache.cordova.geolocation"/>
    <gap:plugin name="org.apache.cordova.dialogs"/>
    <gap:plugin name="org.apache.cordova.inappbrowser"/>
    <gap:plugin name="org.apache.cordova.media"/>
    <gap:plugin name="org.apache.cordova.media-capture"/>
    <gap:plugin name="org.apache.cordova.network-information"/>
    <gap:plugin name="com.phonegap.plugin.statusbar"/>
    <gap:plugin name="org.apache.cordova.splashscreen"/>
    <gap:plugin name="org.apache.cordova.vibration" version="0.3.11" />

    <gap:plugin name="nl.x-services.plugins.toast" />

    <access origin="*" />
</widget>

