<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cgvak.dinesh.raja.pluginstall" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Pluginstall</name>
    <description>
        An Simple Cordova app to illustrate various Plugins available.
    </description>
    <author email="dineshrajaa.93@gmail.com" href="http://cordova.io">
        Dinesh Raja
    </author>
    <content src="index.html" />
    <access origin="*" />      
    <!--Icons-->
    <icon src="icon.png" platform="ios" width="57" height="57" density="mdpi" />
    <!--Android Icons-->
    <platform name="android">
              <icon src="res/icon/android/ldpi.png" density="ldpi" />
              <icon src="res/icon/android/mdpi.png" density="mdpi" />
              <icon src="res/icon/android/hdpi.png" density="hdpi" />
              <icon src="res/icon/android/xhdpi.png" density="xhdpi" />
     </platform>
     <!--ios Icons-->
     <platform name="ios">
              <!-- iOS 8.0+ -->
              <!-- iPhone 6 Plus  -->
              <icon src="res/icon/ios/icon-60@3x.png" width="180" height="180" />
              <!-- iOS 7.0+ -->
              <!-- iPhone / iPod Touch  -->
              <icon src="res/icon/ios/icon-60.png" width="60" height="60" />
              <icon src="res/icon/ios/icon-60@2x.png" width="120" height="120" />
              <!-- iPad -->
              <icon src="res/icon/ios/icon-76.png" width="76" height="76" />
              <icon src="res/icon/ios/icon-76@2x.png" width="152" height="152" />
              <!-- iOS 6.1 -->
              <!-- Spotlight Icon -->
              <icon src="res/icon/ios/icon-40.png" width="40" height="40" />
              <icon src="res/icon/ios/icon-40@2x.png" width="80" height="80" />
              <!-- iPhone / iPod Touch -->
              <icon src="res/icon/ios/icon-57.png" width="57" height="57" />
              <icon src="res/icon/ios/icon-57@2x.png" width="114" height="114" />
              <!-- iPad -->
              <icon src="res/icon/ios/icon-72.png" width="72" height="72" />
              <icon src="res/icon/ios/icon-72@2x.png" width="144" height="144" />
              <!-- iPhone Spotlight and Settings Icon -->
              <icon src="res/ios/icon-small.png" width="29" height="29" />
              <icon src="res/ios/icon-small@2x.png" width="58" height="58" />
              <!-- iPad Spotlight and Settings Icon -->
              <icon src="res/ios/icon-50.png" width="50" height="50" />
              <icon src="res/ios/icon-50@2x.png" width="100" height="100" />
     </platform>

     <platform name="android">
     	<splash src="res/screen/android/hdpi.png" density="land-hdpi" />
     	<splash src="res/screen/android/ldpi.png" density="land-ldpi" />
     	<splash src="res/screen/android/mdpi.png" density="land-mdpi" />
     	<!--<splash src="res/screen/android/xhdpi.png" density="land-xhdpi" /> -->
     	<splash src="res/screen/android/landscape-xxhdpi.png" density="land-hdpi" />
     	<splash src="res/screen/android/portrait-xxhdpi.png" density="port-xhdpi" />
     </platform>
     <preference name="SplashScreen" value="splash.png" />
     <preference name="SplashScreenDelay" value="10000" />
</widget>
